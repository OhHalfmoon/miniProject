package team5.util;

public class MyRangeException extends RuntimeException{
	
	public MyRangeException() { } //생성자 생성
	
	public MyRangeException(String start, String end) {
		super(start + "~" + end + "자리의 값을 입력하세요");
		
	}

	
	
	
	public class MyRangeException2 extends RuntimeException {
		
		public MyRangeException2() { } //생성자 생성
		
		public MyRangeException2(String limit) {
			super("영문자로 입력해주세요 (한글X)");
			
		}
		
		
	public class MyRangeException3 extends RuntimeException {
		
		public MyRangeException3() { } //생성자 생성
		
		public MyRangeException3(String limit) {
			super("");
			
		}
	
		}
	}
	
}	

